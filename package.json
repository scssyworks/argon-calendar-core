{
  "name": "argon-calendar-core",
  "version": "0.1.5",
  "description": "Argon calendar core provides set of APIs to build your own calendar",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/src/index.d.ts",
  "scripts": {
    "typings": "tsc",
    "build": "npm run typings && rollup-scripts build",
    "lint": "rollup-scripts lint",
    "test": "jest --collect-coverage --detect-open-handles",
    "format": "biome format ./ --write",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scssyworks/argon-calendar-core.git"
  },
  "keywords": ["calendar", "core", "api"],
  "author": "scssyworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scssyworks/argon-calendar-core/issues"
  },
  "homepage": "https://github.com/scssyworks/argon-calendar-core#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rollup-scripts": "^0.0.124"
  },
  "files": ["dist/cjs", "dist/esm", "dist/typings", "LICENSE"]
}
